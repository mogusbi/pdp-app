{
  "name": "pdp-app",
  "version": "1.0.0",
  "description": "",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "create-migration": "typeorm migration:create -n",
    "e2e": "jest --config=e2e/jest.config.js --runInBand --forceExit",
    "environment": "docker-compose -p pdp up",
    "lambda": "serverless offline start",
    "lint": "tslint -p tsconfig.json",
    "serverless": "serverless",
    "start": "nodemon --exec ts-node src/main.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mogusbi/pdp-app.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^5.2.2",
    "aws-serverless-express": "^3.3.5",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "express": "^4.16.4",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/aws-lambda": "^8.10.15",
    "@types/aws-serverless-express": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "@types/supertest": "^2.0.6",
    "@types/webpack-env": "^1.13.6",
    "aws-lambda": "^0.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "serverless": "^1.33.2",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-typescript": "^1.1.5",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/e2e/**/*.ts",
      "!**/src/**/*.dto.ts",
      "!**/src/**/*.entity.ts",
      "!**/src/**/*.model.ts",
      "!**/src/**/*.module.ts",
      "!**/src/**/*.providers.ts"
    ],
    "coveragePathIgnorePatterns": [
      "index.ts",
      "main.ts",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts|js)?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
