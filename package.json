{
  "name": "pdp-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "create-migration": "typeorm migration:create -n",
    "environment": "docker-compose -p pdp up",
    "start": "nodemon --exec ts-node src/main.ts",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@ssh.dev.azure.com:v3/PayrollOnlineServices/OnlineServices/pdp-app"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^5.2.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "@types/supertest": "^2.0.6",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/e2e/**/*.ts",
      "!**/src/**/*.dto.ts",
      "!**/src/**/*.entity.ts",
      "!**/src/**/*.model.ts",
      "!**/src/**/*.module.ts",
      "!**/src/**/*.providers.ts"
    ],
    "coveragePathIgnorePatterns": [
      "index.ts",
      "main.ts",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts|js)?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
